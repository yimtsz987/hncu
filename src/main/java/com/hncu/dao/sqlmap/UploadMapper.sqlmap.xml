<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hncu.dao.mapper.UploadMapper">
    <insert id="uploadDate" parameterType="Upload">
        INSERT INTO download (title,issuer,issueDate,description,receive,uploadFile,uploadPath,uploadFileOldName) VALUES (#{title},#{issuer.id},#{issueDate},#{description},#{receive},#{uploadFile},#{uploadPath},#{uploadFileOldName})
    </insert>
</mapper>